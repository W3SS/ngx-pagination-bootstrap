{
  "name": "ng-bootstrap-pagination",
  "version": "1.0.0",
  "description": "A bootstrap(4) based angular(4+) pagination module.",
  "main": "ng-bootstrap-pagination.umd.js",
  "jsnext:main": "ng-bootstrap-pagination.esm.js",
  "module": "ng-bootstrap-pagination.esm.js",
  "types": "index.d.ts",
  "scripts": {
    "copy": "node tools/copy-package.js && node tools/cleanup.js",
    "bundle": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js && tsc && ngc",
    "postbundle": "node tools/removecode.js",
    "build": "npm run bundle && npm run copy"
  },
  "author": {"name":"Manish Kumar","email":"manishjanky@gmail.com"},
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.9"
  },
  "devDependencies": {
    "@angular/cli": "1.6.0",
    "@angular/compiler-cli": "^5.0.0",
    "@types/node": "^7.0.14",
    "del": "^2.2.2",
    "node-sass": "^4.7.2",
    "rollup": "^0.45.2",
    "rollup-plugin-angular": "^0.4.12",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "typescript": "^2.3.2"
  }
}
