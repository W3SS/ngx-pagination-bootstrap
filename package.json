  "description": "A bootstrap(4) based angular(4+) pagination module.",
  "main": "ngx-pagination-bootstrap.umd.js",
  "jsnext:main": "ngx-pagination-bootstrap.esm.js",
  "module": "ngx-pagination-bootstrap.esm.js",
  "types": "index.d.ts",
  "scripts": {
    "copy": "node tools/copy-package.js && node tools/copy-readme.js && node tools/cleanup.js",
    "bundle": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js && tsc && ngc",
    "postbundle": "node tools/removecode.js",
    "build": "npm run bundle && npm run copy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manishjanky/ngx-pagination-bootstrap.git"
  },
  "keywords": [
